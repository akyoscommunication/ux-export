{
  "name": "akyos/ux-export",
  "type": "symfony-bundle",
  "description": "A Symfony bundle for export in live Component",
  "keywords": ["symfony", "PHP"],
  "minimum-stability": "stable",
  "license": "proprietary",
  "authors": [
    {
      "name": "Johan JOULKVA",
      "email": "johan.akyos@gmail.com"
    }
  ],
  "require": {
    "php": "^8.1",
    "symfony/ux-live-component": "^2.21",
    "symfony/serializer-pack": "^1.3",
    "phpoffice/phpspreadsheet": "^3.9",
    "symfony/dependency-injection": "6.*|7.*",
    "symfony/config": "6.*|7.*"
  },
  "autoload": {
    "psr-4": { "Akyos\\UXExportBundle\\": "src/" },
    "exclude-from-classmap": [
      "/Tests/"
    ]
  },
  "require-dev": {
    "phpunit/phpunit": "^10.3"
  },
  "autoload-dev": {
    "psr-4": {
      "Akyos\\UXExportBundle\\Tests\\": "tests/"
    }
  },
  "scripts": {
    "test": "phpunit"
  }
}
